<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE ="false"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet/MOM and JMS</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SERVER"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
    >
      <ITEMS DEFAULT="WEBSPHERE">
        <ITEM NAME="ACTIVEMQ" VALUE="ActiveMQ"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="STATION"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT>"Talend1"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERADDRESS"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERPORT"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT>61616</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="MQ_DERVIERS" FIELD="TEXT" NUM_ROW="30" SHOW="false">
		<DEFAULT>activemq-all-5.1.0.jar</DEFAULT>
	</PARAMETER>
    
     <PARAMETER
      NAME="FROM"
      FIELD="TEXT"
      SHOW="true"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'JBOSS')OR (SERVER == 'ACTIVEMQ')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
      
     <PARAMETER
      NAME="TO"
      FIELD="TEXT"
      SHOW="true"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'JBOSS')OR (SERVER == 'ACTIVEMQ')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="MSGTYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'JBOSS')OR (SERVER == 'ACTIVEMQ')"
    >
      <ITEMS DEFAULT="TOPIC">
        <ITEM NAME="TOPIC"   VALUE="Topic" />
        <ITEM NAME="QUEUE"   VALUE="Queue" />
      </ITEMS>
    </PARAMETER>
    
  <PARAMETER
      NAME="CHANNEL"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'WEBSPHERE')" >
   	<DEFAULT>"DC.SVRCONN"</DEFAULT>
    </PARAMETER>
    
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="40"
	  
    >
    </PARAMETER>
   
	
  </PARAMETERS>

  <ADVANCED_PARAMETERS>	
	
	
	 <PARAMETER
      NAME="DELIVERY_MODE"
      FIELD="CLOSED_LIST"
      NUM_ROW="10"
      REQUIRED="true">	  
	  <ITEMS DEFAULT="N_PERSISTENT">
        <ITEM NAME="N_PERSISTENT"   VALUE="N_PERSISTENT" />
        <ITEM NAME="PERSISTENT"   VALUE="PERSISTENT" />
      </ITEMS>	
	</PARAMETER>
	
	<PARAMETER NAME="IS_TRANSACTED" FIELD="CHECK" REQUIRED="false" NUM_ROW="20"  SHOW="false">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ACKNOWLEDGMENT_MODE" FIELD="CLOSED_LIST" NUM_ROW="30" SHOW="false">
      <ITEMS DEFAULT="AUTO_ACKNOWLEDGE">
        <ITEM NAME="AUTO_ACKNOWLEDGE"   VALUE="AUTO_ACKNOWLEDGE" />
        <ITEM NAME="CLIENT_ACKNOWLEDGE"   VALUE="CLIENT_ACKNOWLEDGE" />
        <ITEM NAME="DUPS_OK_ACKNOWLEDGE"   VALUE="DUPS_OK_ACKNOWLEDGE" />
      </ITEMS>
    </PARAMETER>
    
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
   <IMPORTS>
       <IMPORT NAME="JNP-CLIENT" MODULE="jnp-client.jar" REQUIRED="true" />
       <IMPORT NAME="JAVA_SSIST" MODULE="javassist.jar" REQUIRED="true" /> 
       <IMPORT NAME="TROVE" MODULE="trove.jar" REQUIRED="true" />   
       <IMPORT NAME="LOG4J" MODULE="log4j-1.2.13.jar" REQUIRED="false" />
       <IMPORT NAME="ACTIVEMQ LIB" MODULE="activemq-all-5.5.0.jar" />
       <IMPORT NAME="DAS_SERVER" MODULE="opendas-server.jar" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>
