<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE ="false"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet/MOM and JMS</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="5"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="KEEPLISTENING"
      FIELD="CHECK"
      SHOW="true"
      NUM_ROW="10"
      SHOW_IF="(USEMAX == 'false')"
    >
    <DEFAULT>true</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="STARTSERVER"
      FIELD="CHECK"
      NUM_ROW="60"
    >
    <DEFAULT >false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USEMAX"
      FIELD="CHECK"
      SHOW_IF="(KEEPLISTENING == 'false')"
      NUM_ROW="15"
    >
    <DEFAULT >false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TIMEOUT"
      FIELD="TEXT"
      SHOW_IF="(KEEPLISTENING == 'false' and USEMAX == 'false')"
      NUM_ROW="12"
    >
    <DEFAULT>1</DEFAULT>
    </PARAMETER>

<PARAMETER
      NAME="MAXMSG"
      FIELD="TEXT"
      SHOW_IF="(KEEPLISTENING == 'false' and USEMAX == 'true')"
      NUM_ROW="17"
    >
    <DEFAULT>1000</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SERVER"
      FIELD="CLOSED_LIST"
      NUM_ROW="25"
    >
      <ITEMS DEFAULT="ACTIVEMQ">
        <ITEM NAME="ACTIVEMQ" VALUE="ActiveMQ"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="STATION"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT>"Talend1"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SERVERADDRESS"
      FIELD="TEXT"
      NUM_ROW="25"
    >
	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERPORT"
      FIELD="TEXT"
      NUM_ROW="25"
    >
	<DEFAULT >"61616"</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="MQ_DERVIERS" FIELD="TEXT" NUM_ROW="30" SHOW="false">
		<DEFAULT>activemq-all-5.1.0.jar</DEFAULT>
	</PARAMETER>

   <PARAMETER
      NAME="FROM"
      FIELD="TEXT"
      SHOW="true"
      NUM_ROW="35"
    ><DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="MSGTYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="35"
    >
      <ITEMS DEFAULT="TOPIC">
        <ITEM NAME="TOPIC"   VALUE="Topic" />
        <ITEM NAME="QUEUE"   VALUE="Queue" />
      </ITEMS>
    </PARAMETER>
    

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="45"
    >
    </PARAMETER>
 
  </PARAMETERS>
  
	<ADVANCED_PARAMETERS>	
	
	<PARAMETER NAME="IS_TRANSACTED" FIELD="CHECK" REQUIRED="false" NUM_ROW="20" SHOW="false">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

		<PARAMETER NAME="ACKNOWLEDGMENT_MODE" FIELD="CLOSED_LIST" NUM_ROW="30">
	      <ITEMS DEFAULT="AUTO_ACKNOWLEDGE">
	        <ITEM NAME="AUTO_ACKNOWLEDGE"   VALUE="AUTO_ACKNOWLEDGE" />
	        <ITEM NAME="CLIENT_ACKNOWLEDGE"   VALUE="CLIENT_ACKNOWLEDGE" />
	        <ITEM NAME="DUPS_OK_ACKNOWLEDGE"   VALUE="DUPS_OK_ACKNOWLEDGE" />
	      </ITEMS>
	    </PARAMETER>
	    
	</ADVANCED_PARAMETERS>
	
  <CODEGENERATION>
     <IMPORTS>
	     <IMPORT NAME="JNP-CLIENT" MODULE="jnp-client.jar" REQUIRED="true" />
	     <IMPORT NAME="JBOSS MESSAGING CLIENT" MODULE="jboss-messaging-client.jar" REQUIRED="true" />
	     <IMPORT NAME="JAVA_SSIST" MODULE="javassist.jar" REQUIRED="true" /> 
	     <IMPORT NAME="TROVE" MODULE="trove.jar" REQUIRED="true" />   
	     <IMPORT NAME="LOG4J" MODULE="log4j-1.2.13.jar" REQUIRED="true" />
	     <IMPORT NAME="das_server" MODULE="das_server.jar" REQUIRED="true" />
	     <IMPORT NAME="TOPLINK" MODULE="toplink-essentials.jar" REQUIRED="true" />
	     <IMPORT NAME="ACTIVEMQ LIB" MODULE="activemq-all-5.5.0.jar" REQUIRED_IF="(USE_CONNECTION=='false' AND SERVER == 'ACTIVEMQ')" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>
